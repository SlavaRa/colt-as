<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="codeOrchestra.colt.as.ui.propertyTabPane.projectPaths.ProjectPathsFormController">
  <VBox AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
    <children>
      <fx:include fx:id="sourcePaths" source="paths_form.fxml" />
      <fx:include fx:id="libraryPaths" source="paths_form.fxml" />
      <fx:include fx:id="assetsPaths" source="paths_form.fxml" />
      <HBox maxWidth="1.7976931348623157E308" spacing="10.0">
        <children>
          <Label minWidth="-Infinity" text="HTML Template:" />
          <TextField fx:id="templateTF" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
          <Button minWidth="-Infinity" mnemonicParsing="false" text="Browse..." onAction="#browseHandler"/>
        </children>
        <padding>
          <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
        </padding>
      </HBox>
    </children>
  </VBox>
</AnchorPane>