<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<Tab fx:id="project_paths" fx:controller="codeOrchestra.colt.as.ui.propertyTabPane.projectPaths.ProjectPathsController" text="Project Paths" xmlns:fx="http://javafx.com/fxml">
  <content>
    <VBox>
      <children>
        <fx:include fx:id="sourcePaths" source="paths_form.fxml" />
        <fx:include fx:id="libraryPaths" source="paths_form.fxml" />
        <fx:include fx:id="assetsPaths" source="paths_form.fxml" />
        <HBox maxWidth="1.7976931348623157E308" spacing="10.0">
          <children>
            <Label minWidth="-Infinity" text="HTML Template:" />
            <TextField fx:id="templateTF" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
            <Button minWidth="-Infinity" mnemonicParsing="false" text="Browse..." onAction="#browseHandler"/>
          </children>
          <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
          </padding>
        </HBox>
      </children>
    </VBox>
  </content>
</Tab>
